                        document.getElementById('mobile_number_').textContent = "Mobile number not found.";
                    }

                    // Call fetchText function on page load (optional)
                    // window.onload = fetchText; // Uncomment to fetch text automatically

                    let minutes = 15;
                    let seconds = 0;

                    const countdown = setInterval(() => {
                        const timerElement = document.getElementById('timer');

                        if (seconds === 0) {
                            if (minutes === 0) {
                                clearInterval(countdown);
                                timerElement.textContent = "Countdown ended!";
                                return;
                            }
                            minutes--;
                            seconds = 59;
                        } else {
                            seconds--;
                        }

                        timerElement.textContent = `${minutes.toString().padStart(2, "0")}:${seconds.toString().padStart(2, "0")}`;
                    }, 1000); // Start the timer on page load
